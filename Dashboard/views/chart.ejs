   <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      google.load("visualization", "1.1", {packages:["bar"]});
      google.setOnLoadCallback(drawStuff);

      function drawStuff() {
        var data = new google.visualization.arrayToDataTable([
          ['Carrier', 'Avg. Download Speed'],
          ["T-Mobile", <%=networkDetails[0]%>],
          ["AT&T", <%=networkDetails[1]%>],
          ["Verizon", <%=networkDetails[2]%>],
          ["MetroPCS", <%=networkDetails[3]%>],
          ["CricKet", <%=networkDetails[4]%>],
        ]);

        var options = {
          title: 'Network Rank',
          width: 900,
          legend: { position: 'none' },
          chart: { title: 'Network Rank',
                   subtitle: '' },
          bars: 'horizontal', // Required for Material Bar Charts.
          axes: {
            x: {
              0: { side: 'top', label: 'Avg. Download Speed (in Kbps)'} // Top x-axis.
            }
          },
          bar: { groupWidth: "90%" }
        };

        var chart = new google.charts.Bar(document.getElementById('top_x_div'));
        chart.draw(data, options);
      };
    </script>



